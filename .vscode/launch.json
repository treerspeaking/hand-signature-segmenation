{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Hand Segmentation Inference",
            "type": "python",
            "request": "launch",
            "module": "inference.inference_folder",
            "args": [
                "--checkpoint",
                "/home/treerspeaking/src/python/hand_seg/logs/hand_segmentation/version_57/checkpoints/best_hand_segmentation_epoch=44_val/epoch_iou=0.0000.ckpt",
                "--folder",
                "/home/treerspeaking/src/python/hand_seg/encoded_data",
                "--output",
                "./inference/out-unet-base"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
        {
            "name": "debug train.py",
            "type": "debugpy",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "args": [
                "--config",
                "/home/treerspeaking/src/python/hand_seg/config/unet.yaml",
            ],
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}